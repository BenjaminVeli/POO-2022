package poo;
public class Proyecto {
	
	private int codigo;
	private String descripci贸n;
	private	int  cantidad;
	private double  precioUnitario;
	
	
	public Proyecto(int cod,String desc,int can,double prec) {
		codigo = cod;
		descripci贸n = desc;
		cantidad = can;
		precioUnitario = prec;
	}
	
	public void setCodigo(int cod) {
		this.codigo= cod;
	}
	public int getCodigo() {
		return this.codigo;
	}
	
	public String getDescripci贸n() {
		return descripci贸n;
	}
	
	public void setCantidad(int Cantidad) {
		cantidad = Cantidad;
	}
	
	public double precioTotal() {
		return cantidad * precioUnitario;
	}
	
	public double igv() {
		return precioTotal()*0.13;
	}
	
	public double total() {
		return precioTotal() - igv();
	}
}